{
  "name": "zwavejs2mqtt",
  "version": "4.0.1",
  "bin": {
    "zwavejs2mqtt": "bin/www"
  },
  "description": "Zwave To MQTT Gateway",
  "author": "Daniel Lando <daniel.sorridi@gmail.com>",
  "private": false,
  "license": "MIT",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zwave-js/zwavejs2mqtt"
  },
  "bugs": {
    "url": "https://github.com/zwave-js/zwavejs2mqtt/issues"
  },
  "keywords": [
    "mqtt",
    "zwave",
    "control-panel",
    "gateway",
    "hass",
    "homeassistant",
    "iot",
    "nodejs",
    "vue",
    "vuetify"
  ],
  "pkg": {
    "scripts": [
      "lib/**/*.js",
      "config/**/*.js",
      "hass/**/*.js",
      "app.js"
    ],
    "assets": [
      "views/**/*",
      "static/**/*",
      "dist/**/*"
    ]
  },
  "scripts": {
    "dev": "webpack serve --inline --progress --host 0.0.0.0 --config build/webpack.dev.conf.js",
    "dev-https": "SERVER_SSL='true' npm run dev",
    "dev:server": "nodemon --inspect bin/www",
    "dev-https:server": "HTTPS='true' nodemon --inspect bin/www",
    "start": "node bin/www",
    "lint": "npm-run-all lint:*",
    "lint-fix": "npm-run-all lint-fix:*",
    "lint:eslint": "eslint --ext .js,.vue .",
    "lint-fix:eslint": "eslint --fix --ext .js,.vue .",
    "lint:markdownlint": "markdownlint '**/*.md'",
    "lint-fix:markdownlint": "markdownlint --fix '**/*.md'",
    "lint:prettier": "prettier-standard --check",
    "lint-fix:prettier": "prettier-standard --format",
    "test": "npm-run-all test:*",
    "test:server": "mocha",
    "test:ui": "mocha -r @babel/register src/modules",
    "docs": "docsify serve ./docs",
    "coverage": "c8 --clean mocha",
    "record-coverage": "c8 report --reporter=text-lcov --all --exclude=test/* --exclude=.git --exclude=.eslintrc.js --exclude=.postcssrc.js --exclude=wallaby.js > ./coverage/lcov.info",
    "test-watch-cover": "nodemon --exec c8 npm test",
    "test-watch": "nodemon --exec npm test",
    "build": "node build/build.js",
    "pkg": "sudo chmod +x package.sh && ./package.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "changelog-init": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "read -p 'GITHUB_TOKEN: ' GITHUB_TOKEN && export GITHUB_TOKEN=$GITHUB_TOKEN && release-it"
  },
  "release-it": {
    "github": {
      "release": true,
      "assets": [
        "pkg/*.zip"
      ]
    },
    "git": {
      "tagName": "v${version}",
      "requireCleanWorkingDir": false
    },
    "hooks": {
      "before:release": "./package.sh --release"
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "optionalDependencies": {
    "@release-it/conventional-changelog": "^2.0.1",
    "@types/node": "^14.14.34",
    "c8": "^7.6.0",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "mocha": "^8.3.2",
    "node-notifier": "^9.0.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.9",
    "portfinder": "^1.0.28",
    "prettier-standard": "^16.4.1",
    "proxyquire": "^2.1.3",
    "release-it": "^14.4.1",
    "rewire": "^5.0.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@zwave-js/server": "^1.5.0",
    "ansi_up": "^5.0.0",
    "app-root-path": "^3.0.0",
    "archiver": "^5.3.0",
    "axios": "^0.21.1",
    "axios-progress-bar": "^1.2.0",
    "body-parser": "~1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "d3": "^6.6.0",
    "dagre-d3": "^0.6.4",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "express-session": "^1.17.1",
    "fs-extra": "^9.1.0",
    "hammerjs": "^2.0.8",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "mqtt": "^4.2.6",
    "mqtt-nedb-store": "^0.1.1",
    "native-url": "^0.3.4",
    "nedb": "^1.8.0",
    "pem": "^1.14.4",
    "prismjs": "^1.23.0",
    "serialport": "^9.0.7",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0",
    "svg-pan-zoom": "^3.6.1",
    "vue": "^2.6.12",
    "vue-prism-editor": "^1.2.2",
    "vue-router": "^3.5.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.4.8",
    "vuex": "^3.6.2",
    "winston": "^3.3.3",
    "zwave-js": "^7.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.16",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "chalk": "^4.1.1",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.1",
    "deepmerge": "^4.2.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "lodash": "^4.17.21",
    "markdownlint-cli": "^0.27.1",
    "material-design-icons-iconfont": "^6.1.0",
    "mini-css-extract-plugin": "^1.6.0",
    "ora": "^5.4.0",
    "postcss": "^8.2.13",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.34.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
